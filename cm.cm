getTransformationGridData(): void {
    this.httpApiService.fetchTransformationDependentsData().subscribe((res: any) => {
      this.transformationDetails = res; // Store transformation data
      if (res.inputFieldDataList && Array.isArray(res.inputFieldDataList)) {
        this.inputFieldDataList = res.inputFieldDataList; // Store input fields data
      }
    });
  }
