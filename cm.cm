loadJiraFieldData() {
    const requestData = { /* Your request parameters */ }; // Define your request data structure
    this.lighthouseApiService.fetchJiraDetails(requestData).subscribe(
        data => {
            console.log('JIRA DATA:', data);
            this.jiraFieldData = data;
        },
        error => {
            console.log('Error loading JIRA data:', error);
        }
    );
}
